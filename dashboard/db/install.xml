<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="blocks/dashboard/db" VERSION="20111105" COMMENT="XMLDB file for Moodle blocks/dashboard"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="dashboard_cache" COMMENT="Default comment for dashboard, please edit me" NEXT="dashboard_cache_data">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" NEXT="access"/>
        <FIELD NAME="access" TYPE="char" LENGTH="10" NOTNULL="true" DEFAULT="moodle" SEQUENCE="false" ENUM="false" PREVIOUS="id" NEXT="querykey"/>
        <FIELD NAME="querykey" TYPE="char" LENGTH="32" NOTNULL="true" SEQUENCE="false" ENUM="false" COMMENT="Stores hashcode for the query as an index key" PREVIOUS="access" NEXT="timereloaded"/>
        <FIELD NAME="timereloaded" TYPE="char" LENGTH="32" NOTNULL="true" SEQUENCE="false" ENUM="false" COMMENT="Stores hashcode for the query as an index key" PREVIOUS="querykey"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="access_ix" UNIQUE="false" FIELDS="access" NEXT="querykey_ix"/>
        <INDEX NAME="querykey_ix" UNIQUE="false" FIELDS="querykey" COMMENT="Fast sort of query sets" PREVIOUS="access_ix"/>
      </INDEXES>
    </TABLE>
    <TABLE NAME="dashboard_cache_data" COMMENT="Default comment for dashboard, please edit me" PREVIOUS="dashboard_cache" NEXT="dashboard_geo_cache">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" NEXT="access"/>
        <FIELD NAME="access" TYPE="char" LENGTH="10" NOTNULL="true" DEFAULT="moodle" SEQUENCE="false" ENUM="false" PREVIOUS="id" NEXT="querykey"/>
        <FIELD NAME="querykey" TYPE="char" LENGTH="32" NOTNULL="true" SEQUENCE="false" ENUM="false" COMMENT="Stores hashcode for the query as an index key" PREVIOUS="access" NEXT="recordid"/>
        <FIELD NAME="recordid" TYPE="char" LENGTH="60" NOTNULL="true" SEQUENCE="false" ENUM="false" PREVIOUS="querykey" NEXT="record"/>
        <FIELD NAME="record" TYPE="text" LENGTH="small" NOTNULL="false" SEQUENCE="false" ENUM="false" COMMENT="serialized data" PREVIOUS="recordid"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="access_ix" UNIQUE="false" FIELDS="access" NEXT="querykey_ix"/>
        <INDEX NAME="querykey_ix" UNIQUE="false" FIELDS="querykey" COMMENT="Fast sort of query sets" PREVIOUS="access_ix"/>
      </INDEXES>
    </TABLE>
    <TABLE NAME="dashboard_geo_cache" COMMENT="Caches geo localisation data" PREVIOUS="dashboard_cache_data">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" NEXT="address"/>
        <FIELD NAME="address" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" ENUM="false" PREVIOUS="id" NEXT="regioncode"/>
        <FIELD NAME="regioncode" TYPE="char" LENGTH="16" NOTNULL="false" SEQUENCE="false" ENUM="false" COMMENT="The Geolocalisation region" PREVIOUS="address" NEXT="latlng"/>
        <FIELD NAME="latlng" TYPE="char" LENGTH="20" NOTNULL="false" SEQUENCE="false" ENUM="false" COMMENT="The lat,long data" PREVIOUS="regioncode"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>