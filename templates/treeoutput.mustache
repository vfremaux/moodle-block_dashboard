{{!
    @template block-dashboard/treeoutput

    Block template for treeview output params form

    Classes required for JS:
    * none

}}
<!-- block_dashboard/treeoutput -->
<fieldset id="dashboardsettings-panel-treeviewparams" class="dashboardsettings-panel off">
<legend>{{strtreeviewparams}}</legend>
<table class="dashboard-setup">

    <tr valign="top">
        <td>
            <b>{{{strconfigparent}}}: {{{helpiconconfigparent}}}</b>
        </td>

        <td>
            <input type="text" name="parentserie" size="20" value="{{{parentserie}}}" />
        </td>
    </tr>

    <tr valign="top">
        <td>
            <b>{{{strconfigtreeoutput}}}: {{{helpiconconfigtreeoutput}}}</b>
        </td>

        <td>
            <input type="text" name="treeoutput" size="30" value="{{{treeoutput}}}" />
        </td>
    </tr>

    <tr valign="top">
        <td>
            <b>{{{strconfigtreeoutputformats}}}: {{{helpiconconfigformatting}}}</b>
        </td>

        <td>
            <input type="text" name="treeoutputformats" size="30" value="{{{treeoutputformats}}}" />
        </td>
    </tr>

</table>
</fieldset>
<!-- /block_dashboard/treeoutput -->
